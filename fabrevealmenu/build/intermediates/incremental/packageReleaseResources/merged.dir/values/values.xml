<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorBlack">#000000</color>
    <color name="colorOverlayDark">#AA000000</color>
    <color name="colorOverlayLight">#40000000</color>
    <color name="colorWhite">#ffffff</color>
    <dimen name="card_radius">3dp</dimen>
    <dimen name="column_size">65dp</dimen>
    <dimen name="column_size_small">55dp</dimen>
    <dimen name="extra_margin">7dp</dimen>
    <dimen name="extra_margin_small">3dp</dimen>
    <dimen name="fab_margin">2dp</dimen>
    <dimen name="menu_min_width">200dp</dimen>
    <dimen name="menu_min_width_small">150dp</dimen>
    <dimen name="menu_size_horizontal">38dp</dimen>
    <dimen name="menu_size_horizontal_small">30dp</dimen>
    <dimen name="menu_size_vertical">30dp</dimen>
    <dimen name="menu_size_vertical_small">24dp</dimen>
    <dimen name="menu_text_padding">12dp</dimen>
    <dimen name="menu_text_size">18sp</dimen>
    <dimen name="menu_text_size_horizontal">12sp</dimen>
    <dimen name="menu_text_size_horizontal_small">10sp</dimen>
    <dimen name="menu_text_size_small">14sp</dimen>
    <dimen name="row_horizontal_size">70dp</dimen>
    <dimen name="row_horizontal_size_small">60dp</dimen>
    <dimen name="shape_padding">5dp</dimen>
    <integer name="horizontal_menu_max_lines">1</integer>
    <declare-styleable name="FABRevealMenu">
        <attr format="color" name="menuBackgroundColor"/>
        <attr format="reference" name="menuCustomView"/>
        <attr format="reference" name="menuRes"/>
        <attr format="enum" name="menuDirection">
            <enum name="left" value="0"/>
            <enum name="up" value="1"/>
            <enum name="right" value="2"/>
            <enum name="down" value="3"/>
        </attr>
        <attr format="boolean" name="showOverlay"/>
        <attr format="color" name="overlayBackground"/>

        <attr format="boolean" name="showTitle"/>
        <attr format="boolean" name="showIcon"/>
        <attr format="color" name="menuTitleTextColor"/>
        <attr format="color" name="menuTitleDisabledTextColor"/>
        <attr format="boolean" name="animateItems"/>
        <attr format="enum" name="menuSize">
            <enum name="normal" value="0"/>
            <enum name="small" value="1"/>
        </attr>

        <attr format="reference" name="menuTitleFontFamily"/>
    </declare-styleable>
</resources>